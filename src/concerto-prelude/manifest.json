{
	"name": "Concerto",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Concerto Player application for Google Chrome.",
	"icons": {
		"16": "img/icon_16.png",
		"128": "img/icon_128.png"
	},
	
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	
	"permissions": [
		"<all_urls>",
		"geolocation",
		"tts",
		"background"
	],
	"offline_enabled": true
}